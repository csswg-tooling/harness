Options +MultiViews

AddType application/javascript .js
AddHandler application/x-httpd-php .js



RewriteEngine On
RewriteBase	/harness

RewriteRule ^suite/(.+)/ua/([^/]*)(.*)$                 suite/$1$3?u=$2                [DPI,NC,QSA]
RewriteRule ^suite/([^/]*)(/?)$                         testsuite?s=$1                 [L,NC,QSA]

RewriteRule ^agent/([^/]*)(/?)$                         useragent?s=$1                 [L,NC,QSA]

RewriteRule ^test/(.+)/all/(.*)$                        test/$1/$2?o=0                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/ua/([^/]*)(.*)$                  test/$1$3?u=$2                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/ref/([^/]*)(.*)$                 test/$1$3?ref=$2               [DPI,NC,QSA]
RewriteRule ^test/(.+)/format/([^/]*)(.*)$              test/$1$3?f=$2                 [DPI,NC,QSA]
RewriteRule ^test/([^/]*)/single/([^/]*)(/?)$           testcase?s=$1&c=$2&o=1         [L,NC,QSA]
RewriteRule ^test/([^/]*)/section/([^/]*)/([^/]*)(/?)$  testcase?s=$1&sec=$2&i=$3&o=1  [L,NC,QSA]
RewriteRule ^test/([^/]*)/([^/]+)(/?)$                  testcase?s=$1&i=$2&o=1         [L,NC,QSA]
RewriteRule ^test/([^/]*)(/?)$                          testcase?s=$1&o=1              [L,NC,QSA]

RewriteRule ^done/(.+)/ua/([^/]*)(.*)$                  done/$1$3?u=$2                 [DPI,NC,QSA]
RewriteRule ^done/([^/]*)(/?)$                          success?s=$1                   [L,NC,QSA]

# home page, keep this last
RewriteRule ^ua/([^/]*)(/?)$                            index?u=$1                     [L,NC,QSA]
