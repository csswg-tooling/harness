Options +MultiViews

AddType application/javascript .js
AddHandler application/x-httpd-php .js



RewriteEngine On
RewriteBase	/harness

RewriteRule ^suite/(.+)/ua/([^/]*)(.*)$                 suite/$1$3?u=$2                [DPI,NC,QSA]
RewriteRule ^suite/([^/]*)(/?)$                         testsuite?s=$1                 [L,NC,QSA]

RewriteRule ^agent/([^/]*)(/?)$                         useragent?s=$1                 [L,NC,QSA]

RewriteRule ^test/(.+)/all/(.*)$                        test/$1/$2?o=0                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/alpha/(.*)$                      test/$1/$2?o=0                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/ua/([^/]*)(.*)$                  test/$1$3?u=$2                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/ref/([^/]*)(.*)$                 test/$1$3?ref=$2               [DPI,NC,QSA]
RewriteRule ^test/(.+)/format/([^/]*)(.*)$              test/$1$3?f=$2                 [DPI,NC,QSA]
RewriteRule ^test/(.+)/flag/([^/]*)(.*)$                test/$1$3?fl=$2                [DPI,NC,QSA]
RewriteRule ^test/([^/]*)/single/([^/]*)(/?)$           testcase?s=$1&c=$2&o=1         [L,NC,QSA]
RewriteRule ^test/([^/]*)/section/([^/]*)/([^/]*)(/?)$  testcase?s=$1&sec=$2&i=$3&o=1  [L,NC,QSA]
RewriteRule ^test/([^/]*)/([^/]+)(/?)$                  testcase?s=$1&i=$2&o=1         [L,NC,QSA]
RewriteRule ^test/([^/]*)(/?)$                          testcase?s=$1&o=1              [L,NC,QSA]

RewriteRule ^done/(.+)/ua/([^/]*)(.*)$                  done/$1$3?u=$2                 [DPI,NC,QSA]
RewriteRule ^done/([^/]*)(/?)$                          success?s=$1                   [L,NC,QSA]

RewriteRule ^review/(.+)/ua/([^/]*)(.*)$                review/$1$3?u=$2               [DPI,NC,QSA]
RewriteRule ^review/([^/]*)(/?)$                        reviewresults?s=$1             [L,NC,QSA]
RewriteRule ^review$                                    reviewresults                  [L,NC,QSA]

RewriteRule ^results/(.+)/grouped/(.*)$                 results/$1/$2?o=1              [DPI,NC,QSA]
RewriteRule ^results/(.+)/ua/([^/]*)(.*)$               results/$1$3?u=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/filter/([^/]*)(.*)$           results/$1$3?f=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/date/([^/]*)(.*)$             results/$1$3?m=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/engine/([^/]*)(.*)$           results/$1$3?e=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/engine_version/([^/]*)(.*)$   results/$1$3?v=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/browser/([^/]*)(.*)$          results/$1$3?b=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/browser_version/([^/]*)(.*)$  results/$1$3?bv=$2             [DPI,NC,QSA]
RewriteRule ^results/(.+)/platform/([^/]*)(.*)$         results/$1$3?p=$2              [DPI,NC,QSA]
RewriteRule ^results/(.+)/platform_version/([^/]*)(.*)$ results/$1$3?pv=$2             [DPI,NC,QSA]
RewriteRule ^results/([^/]*)/section/([^/]*)(/?)$       resulttable?s=$1&t=1&sec=$2    [L,NC,QSA]
RewriteRule ^results/([^/]*)/([^/]+)(/?)$               resulttable?s=$1&t=2&c=$2      [L,NC,QSA]
RewriteRule ^results/([^/]*)(/?)$                       resulttable?s=$1&t=0           [L,NC,QSA]
RewriteRule ^results$                                   resulttable                    [L,NC,QSA]

RewriteRule ^details/(.+)/grouped/(.*)$                 details/$1/$2?o=1              [DPI,NC,QSA]
RewriteRule ^details/(.+)/ua/([^/]*)(.*)$               details/$1$3?u=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/date/([^/]*)(.*)$             details/$1$3?m=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/engine/([^/]*)(.*)$           details/$1$3?e=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/engine_version/([^/]*)(.*)$   details/$1$3?v=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/browser/([^/]*)(.*)$          details/$1$3?b=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/browser_version/([^/]*)(.*)$  details/$1$3?bv=$2             [DPI,NC,QSA]
RewriteRule ^details/(.+)/platform/([^/]*)(.*)$         details/$1$3?p=$2              [DPI,NC,QSA]
RewriteRule ^details/(.+)/platform_version/([^/]*)(.*)$ details/$1$3?pv=$2             [DPI,NC,QSA]
RewriteRule ^details/([^/]*)/section/([^/]*)(/?)$       detailtable?s=$1&t=1&sec=$2    [L,NC,QSA]
RewriteRule ^details/([^/]*)/([^/]+)(/?)$               detailtable?s=$1&t=2&c=$2      [L,NC,QSA]
RewriteRule ^details/([^/]*)(/?)$                       detailtable?s=$1&t=0           [L,NC,QSA]
RewriteRule ^details$                                   detailtable                    [L,NC,QSA]

# home page, keep this last
RewriteRule ^ua/([^/]*)(/?)$                            index?u=$1                     [L,NC,QSA]
